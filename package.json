{
  "name": "kv-new-react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^app/(.*)$": "<rootDir>src/js/$1"
    },
    "collectCoverageFrom": [
      "src/js/**/*.{js,jsx}",
      "tests/**/*.{js,jsx}",
      "!tests/mocked/*.{js,jsx}",
      "!tests/cucumber/**/*",
      "!tests/mocked/**/*",
      "!src/js/**/*.story*{js,jsx}"
    ],
    "coverageDirectory": "./reports/coverage"
  },
  "scripts": {
    "clean": "del ./dist/*",
    "tests": "cross-env run-p eslint test",
    "test": "cross-env jest -i",
    "reports": "run-p eslint-report coverage-summary",
    "eslint": "cross-env eslint 'src/**/*.js' 'src/**/*.jsx'",
    "eslint-json-summary": "npm run eslint -- -f json > ./reports/eslint.json",
    "eslint-report": "cross-env eslint 'src/**/*.js' 'src/**/*.jsx' -f node_modules/eslint-html-reporter/reporter.js -o reports/eslint.html",
    "coverage": "cross-env jest -i --coverage",
    "coverage-summary": "cross-env istanbul report --root ./ --include reports/coverage/coverage-final.json --dir ./reports json-summary",
    "storybook": "start-storybook -p 9001",
    "serve": "cross-env NODE_CONFIG_DIR=./config/env webpack-dev-server --config ./webpack.config.js",
    "build": "npm run clean && cross-env NODE_CONFIG_DIR=./config/env NODE_ENV=production webpack --config webpack.config.production.js --optimize-minimize"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/keyvaluesoftwaresystems/kv-new-react-boilerplate#readme",
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@babel/runtime": "7.6.3",
    "app-registry": "^1.0.1",
    "axios": "^0.19.0",
    "history": "^4.10.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@storybook/react": "^5.2.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "config": "^3.2.3",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "del-cli": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-html-reporter": "^0.7.4",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "import-glob": "^1.5.0",
    "istanbul": "^0.4.5",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "jest-junit": "^8.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "pre-commit": [
    "tests"
  ]
}
